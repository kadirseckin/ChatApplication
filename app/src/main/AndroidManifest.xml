<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kadirseckin.gochat">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Chatko"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GruplarActivity"></activity>
        <activity android:name=".GrupOlustur" />
        <activity android:name=".DavetListeActivity" />
        <activity android:name=".GrupMesajActivity" />
        <activity android:name=".OzelMesajActivity" />
        <activity android:name=".DigerProfiller" />
        <activity android:name=".Kullanicilar" />
        <activity android:name=".KullaniciProfilActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.onesignal.NotificationOpened.DEFAULT"
            android:value="DISABLE" />
    </application>

</manifest>